        "scope": "cpp",
        "prefix": "find",
        "body": [
            "int f[100005], n, m;",
            "void init()",
            "{",
            "    for (int i = 1; i <= n; i++)",
            "        f[i] = i;",
            "}",
            "int find(int x)",
            "{",
            "    if (f[x] == x)",
            "        return x;",
            "    return f[x] = find(f[x]);",
            "}",
            "void merge(int x, int y)",
            "{",
            "    int fx = find(x);",
            "    int fy = find(y);",
            "    if (fx != fy)",
            "        f[fx] = fy;",
            "}",
        ]
