    "SPFA算法":{
		"scope": "cpp",
		"prefix": "SPFA",
		"body": [
            "void SPFA(int s)",
            "{",
            "    memset(vis, false, sizeof vis);",
            "    memset(dis, 0x3f, sizeof dis);",
            "    dis[s] = 0;",
            "    queue<int> q;",
            "    q.push(s);",
            "    while (q.size())",
            "    {",
            "        int u = q.front();",
            "        q.pop();",
            "        vis[u] = false;",
            "        for (int i = head[u]; i; i = e[i].net)",
            "        {",
            "            int v = e[i].v, w = e[i].w;",
            "            if (dis[v] > dis[u] + w)",
            "            {",
            "                dis[v] = dis[u] + w;",
            "                if (!vis[v])",
            "                    vis[v] = true, q.push(v);",
            "            }",
            "        }",
            "    }",
            "}",
		]
	},
