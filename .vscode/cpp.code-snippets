		"scope": "cpp",
		"prefix": "qpow",
		"body": [
			"ll qpow(ll a,ll b,ll MOD)",
			"{",
			"   ll ans=1;",
			"   while(b)",
			"   {",
			"      if(b&1)",
			"         ans=ans*a%MOD;",
			"      a=a*a%MOD,b>>=1;",
			"   }",
			"   return ans;",
			"}"
