    "高精度加法":{
		"scope": "cpp",
		"prefix": "Hadd",
		"body": [
            "// 高精度加法(a,b为反向数字)",
            "string High_Add(string a, string b)",
            "{",
            "    string str;",
            "    for (int i = a.size(); i < b.size(); i++)",
            "        a += \"0\";",
            "    for (int i = b.size(); i < a.size(); i++)",
            "        b += \"0\";",
            "    int m = 0;",
            "    for (int i = 0; i < a.size(); i++)",
            "    {",
            "        str += (a[i] - '0' + b[i] - '0' + m) % 10 + '0';",
            "        m = (a[i] - '0' + b[i] - '0' + m) / 10;",
            "    }",
            "    if (m != 0)",
            "        str += m + '0';",
            "    // cout << a << '+' << b << '=' << str << endl;",
            "    return str;",
            "}",
            "// 反向输出",
            "void print(string str)",
            "{",
            "    for (int i=str.size()-1;i>=0;i--)",
            "        cout << str[i];",
            "}",
        ]
